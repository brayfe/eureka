<?php

/**
 * @file
 * Contains eureka_footer_content.module..
 */

/**
 * Implements hook_theme().
 */
function eureka_footer_content_theme() {
  $theme['footer_lower'] = [
    'variables' => [
      'ugslogourl' => NULL,
      'ugslogosrc' => NULL,
      'ugslogoalt' => NULL,
      'year' => NULL,
      'policy_link' => NULL,
      'accessibility_link' => NULL,
      'privacy_link' => NULL,
    ],
    'template' => 'footer-lower',
  ];

  $theme['footer_upper'] = [
    'variables' => ['name' => NULL, 'year' => NULL],
    'template' => 'footer-upper',
  ];

  return $theme;
}
